{
  "name": "@solis/frontend",
  "private": true,
  "scripts": {
    "lint": "tslint \"src/**/*.ts?(x)\"",
    "prebuild": "rimraf \"build\"",
    "build": "webpack --env prod",
    "start": "webpack-dev-server --env dev",
    "pregenerate": "apollo-codegen introspect-schema http://localhost:8000/api --output schema.json",
    "generate": "apollo-codegen generate \"src/schema/*.graphql\" --schema schema.json --target typescript-modern --add-typename",
    "postgenerate": "rimraf schema.json && node \"scripts/fetchFragmentTypes\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
    "@fortawesome/fontawesome": "^1.2.0-6",
    "@fortawesome/fontawesome-free-solid": "^5.1.0-3",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "aphrodite": "^2.2.2",
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "dakpan": "^1.2.1",
    "date-fns": "^1.29.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "normalize.css": "^8.0.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-dnd": "^4.0.4",
    "react-dnd-html5-backend": "^4.0.5",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux-form": "^7.2.6",
    "@types/redux-logger": "^3.0.6",
    "apollo-codegen": "^0.19.1",
