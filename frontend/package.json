{
  "name": "@solis/frontend",
  "private": true,
  "scripts": {
    "lint": "tslint \"src/**/*.ts?(x)\"",
    "prebuild": "rimraf \"build\"",
    "build": "webpack --env prod",
    "start": "webpack-dev-server --env dev",
    "pregenerate": "apollo-codegen introspect-schema http://localhost:8000/api --output schema.json",
    "generate": "apollo-codegen generate \"src/schema/*.graphql\" --schema schema.json --target typescript-modern --add-typename",
    "postgenerate": "rimraf schema.json && node \"scripts/fetchFragmentTypes\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "@fortawesome/fontawesome": "^1.2.0-6",
