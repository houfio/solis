    "@fortawesome/react-fontawesome": "0.1.0-11",
    "aphrodite": "^2.2.1",
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.0.9",
    "date-fns": "^1.29.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "normalize.css": "^8.0.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dnd": "^4.0.2",
    "react-dnd-html5-backend": "^4.0.2",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-motive": "^1.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "whatwg-fetch": "^2.0.4"
