{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "jsx-no-multiline-js": false,
    "import-blacklist": [
      true,
      "aphrodite"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-over-type-literal": false,
    "no-console": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "trailing-comma": [
      true,
      "never"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ]
  }
}
